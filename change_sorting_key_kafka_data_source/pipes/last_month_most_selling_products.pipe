TOKEN "most_selling_endpoint_read_4504" READ

NODE actions_by_product_0
SQL >

    SELECT 
        product_id, 
        COUNT(*) AS c,
        toMonth(now() - INTERVAL 1 MONTH) as month
    FROM product_events
    WHERE 
        event_name = 'buy-product' AND 
        __timestamp >= toStartOfMonth(now()) - INTERVAL 1 MONTH
    GROUP BY 
        product_id
    ORDER BY 
        c DESC
